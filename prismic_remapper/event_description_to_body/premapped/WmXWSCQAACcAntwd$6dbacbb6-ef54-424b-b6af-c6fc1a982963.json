  "bookingInformation": [
    {
      "type": "paragraph",
      "content": {
        "text": "Only teachers are able to book this session. ",
        "spans": []
      }
    },
    {
      "type": "paragraph",
      "content": {
        "text": "We will also run one on-demand summer term session between Monday 16 April and Friday 20 July. If you are interested, email youthprogrammes@wellcomecollection.org to suggest three dates in order of preference. We will need at least four weeks’ notice.",
        "spans": []
      }
    }
