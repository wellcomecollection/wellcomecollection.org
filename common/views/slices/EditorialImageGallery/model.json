{
  "id": "editorialImageGallery",
  "type": "SharedSlice",
  "name": "EditorialImageGallery",
  "description": "EditorialImageGallery",
  "legacyPaths": {
    "articles::body::editorialImageGallery": "default",
    "books::body::editorialImageGallery": "default",
    "event-series::body::editorialImageGallery": "default",
    "events::body::editorialImageGallery": "default",
    "exhibitions::body::editorialImageGallery": "default",
    "guides::body::editorialImageGallery": "default",
    "pages::body::editorialImageGallery": "default",
    "places::body::editorialImageGallery": "default",
    "projects::body::editorialImageGallery": "default",
    "seasons::body::editorialImageGallery": "default",
    "series::body::editorialImageGallery": "default",
    "webcomics::body::editorialImageGallery": "default"
  },
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "title": {
          "type": "StructuredText",
          "config": {
            "label": "Title",
            "single": "heading1",
            "useAsTitle": true
          }
        },
        "isFrames": {
          "type": "Boolean",
          "config": {
            "label": "layout in frames",
            "default_value": false,
            "placeholder_true": "true",
            "placeholder_false": "false"
          }
        }
      },
      "items": {
        "image": {
          "type": "Image",
          "config": {
            "label": "Image",
            "thumbnails": [
              {
                "name": "32:15",
                "width": 3200,
                "height": 1500
              },
              {
                "name": "16:9",
                "width": 3200,
                "height": 1800
              },
              {
                "name": "square",
                "width": 3200,
                "height": 3200
              }
            ]
          }
        },
        "caption": {
          "type": "StructuredText",
          "config": {
            "multi": "paragraph,hyperlink,strong,em",
            "label": "Caption"
          }
        },
        "hasRoundedCorners": {
          "type": "Boolean",
          "config": {
            "default_value": false,
            "label": "round image corners"
          }
        }
      }
    }
  ]
}