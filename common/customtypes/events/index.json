{
  "id": "events",
  "label": "Event",
  "repeatable": true,
  "status": true,
  "format": "custom",
  "json": {
    "Event": {
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "single": "heading1",
          "useAsTitle": true
        }
      },
      "format": {
        "type": "Link",
        "config": {
          "label": "Format",
          "select": "document",
          "customtypes": ["event-formats"]
        }
      },
      "locations": {
        "type": "Group",
        "fieldset": "Locations",
        "config": {
          "fields": {
            "location": {
              "type": "Link",
              "config": {
                "label": "Location",
                "select": "document",
                "customtypes": ["places"]
              }
            }
          }
        }
      },
      "isOnline": {
        "type": "Boolean",
        "config": {
          "default_value": false,
          "label": "Happens Online?"
        }
      },
      "availableOnline": {
        "type": "Boolean",
        "config": {
          "default_value": false,
          "label": "Available Online?"
        }
      },
      "times": {
        "type": "Group",
        "fieldset": "Times",
        "config": {
          "fields": {
            "startDateTime": {
              "type": "Timestamp",
              "config": {
                "label": "Start"
              }
            },
            "endDateTime": {
              "type": "Timestamp",
              "config": {
                "label": "End"
              }
            },
            "isFullyBooked": {
              "type": "Select",
              "config": {
                "label": "In-venue fully booked",
                "options": ["yes"]
              }
            },
            "onlineIsFullyBooked": {
              "type": "Select",
              "config": {
                "label": "Online fully booked",
                "options": ["yes"]
              }
            }
          }
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "uid",
          "placeholder": ""
        }
      },
      "bslLeafletVideo": {
        "type": "Embed",
        "config": {
          "label": "BSL leaflet video",
          "placeholder": ""
        }
      },
      "body": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "gifVideo": {
              "type": "SharedSlice"
            },
            "iframe": {
              "type": "SharedSlice"
            },
            "editorialImage": {
              "type": "SharedSlice"
            },
            "editorialImageGallery": {
              "type": "SharedSlice"
            },
            "map": {
              "type": "SharedSlice"
            },
            "contentList": {
              "type": "SharedSlice"
            },
            "embed": {
              "type": "SharedSlice"
            },
            "titledTextList": {
              "type": "SharedSlice"
            },
            "textAndImage": {
              "type": "SharedSlice"
            },
            "text": {
              "type": "SharedSlice"
            },
            "searchResults": {
              "type": "SharedSlice"
            },
            "textAndIcons": {
              "type": "SharedSlice"
            },
            "standfirst": {
              "type": "SharedSlice"
            },
            "tagList": {
              "type": "SharedSlice"
            },
            "quote": {
              "type": "SharedSlice"
            },
            "infoBlock": {
              "type": "SharedSlice"
            },
            "contact": {
              "type": "SharedSlice"
            },
            "collectionVenue": {
              "type": "SharedSlice"
            },
            "audioPlayer": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "Access": {
      "interpretations": {
        "type": "Group",
        "fieldset": "Interpretations",
        "config": {
          "fields": {
            "interpretationType": {
              "type": "Link",
              "config": {
                "label": "Interpretation",
                "select": "document",
                "customtypes": ["interpretation-types"]
              }
            },
            "isPrimary": {
              "type": "Select",
              "config": {
                "label": "Primary interprtation",
                "options": ["yes"]
              }
            },
            "extraInformation": {
              "type": "StructuredText",
              "config": {
                "multi": "paragraph,hyperlink,strong,em",
                "label": "Extra information"
              }
            }
          }
        }
      },
      "audiences": {
        "type": "Group",
        "fieldset": "Audiences",
        "config": {
          "fields": {
            "audience": {
              "type": "Link",
              "config": {
                "label": "Audience",
                "select": "document",
                "customtypes": ["audiences"]
              }
            }
          }
        }
      }
    },
    "Reservation": {
      "ticketSalesStart": {
        "type": "Timestamp",
        "config": {
          "label": "Ticket sales start"
        }
      },
      "bookingEnquiryTeam": {
        "type": "Link",
        "config": {
          "label": "Booking enquiry team",
          "select": "document",
          "customtypes": ["teams"]
        }
      },
      "eventbriteEvent": {
        "type": "Embed",
        "config": {
          "label": "Eventbrite event"
        }
      },
      "thirdPartyBookingName": {
        "type": "Text",
        "config": {
          "label": "Third party booking name"
        }
      },
      "thirdPartyBookingUrl": {
        "type": "Link",
        "config": {
          "label": "Third party booking url",
          "select": "web",
          "customtypes": []
        }
      },
      "bookingInformation": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,hyperlink,strong,em",
          "label": "Extra information"
        }
      },
      "policies": {
        "type": "Group",
        "fieldset": "Policies",
        "config": {
          "fields": {
            "policy": {
              "type": "Link",
              "config": {
                "label": "Policy",
                "select": "document",
                "customtypes": ["event-policies"]
              }
            }
          }
        }
      },
      "hasEarlyRegistration": {
        "type": "Select",
        "config": {
          "label": "Early registration",
          "options": ["yes"]
        }
      },
      "cost": {
        "type": "Text",
        "config": {
          "label": "Cost"
        }
      }
    },
    "Online reservation": {
      "onlineTicketSalesStart": {
        "type": "Timestamp",
        "config": {
          "label": "Ticket sales start"
        }
      },
      "onlineBookingEnquiryTeam": {
        "type": "Link",
        "config": {
          "label": "Booking enquiry team",
          "select": "document",
          "customtypes": ["teams"]
        }
      },
      "onlineEventbriteEvent": {
        "type": "Embed",
        "config": {
          "label": "Eventbrite event"
        }
      },
      "onlineThirdPartyBookingName": {
        "type": "Text",
        "config": {
          "label": "Third party booking name"
        }
      },
      "onlineThirdPartyBookingUrl": {
        "type": "Link",
        "config": {
          "label": "Third party booking url",
          "select": "web",
          "customtypes": []
        }
      },
      "onlineBookingInformation": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,hyperlink,strong,em",
          "label": "Extra information"
        }
      },
      "onlinePolicies": {
        "type": "Group",
        "fieldset": "Policies",
        "config": {
          "fields": {
            "policy": {
              "type": "Link",
              "config": {
                "label": "Policy",
                "select": "document",
                "customtypes": ["event-policies"]
              }
            }
          }
        }
      },
      "onlineHasEarlyRegistration": {
        "type": "Select",
        "config": {
          "label": "Early registration",
          "options": ["yes"]
        }
      },
      "onlineCost": {
        "type": "Text",
        "config": {
          "label": "Cost"
        }
      }
    },
    "Schedule": {
      "schedule": {
        "type": "Group",
        "fieldset": "Events",
        "config": {
          "fields": {
            "event": {
              "type": "Link",
              "config": {
                "label": "Event",
                "select": "document",
                "customtypes": ["events"]
              }
            },
            "isNotLinked": {
              "type": "Select",
              "config": {
                "label": "Suppress link to event",
                "options": ["yes"]
              }
            }
          }
        }
      },
      "backgroundTexture": {
        "type": "Link",
        "config": {
          "label": "Background texture",
          "select": "document",
          "customtypes": ["background-textures"]
        }
      }
    },
    "Contributors": {
      "contributors": {
        "type": "Group",
        "fieldset": "Contributors",
        "config": {
          "fields": {
            "role": {
              "type": "Link",
              "config": {
                "label": "Role",
                "select": "document",
                "customtypes": ["editorial-contributor-roles"]
              }
            },
            "contributor": {
              "type": "Link",
              "config": {
                "label": "Contributor",
                "select": "document",
                "customtypes": ["people", "organisations"]
              }
            },
            "description": {
              "type": "StructuredText",
              "config": {
                "multi": "paragraph,hyperlink,strong,em",
                "label": "Contributor description override"
              }
            }
          }
        }
      },
      "contributorsTitle": {
        "type": "StructuredText",
        "config": {
          "single": "heading1",
          "label": "Contributors heading override"
        }
      }
    },
    "Promo": {
      "promo": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "editorialImage": {
              "type": "Slice",
              "fieldset": "Editorial image",
              "config": {
                "label": "Editorial image"
              },
              "non-repeat": {
                "caption": {
                  "type": "StructuredText",
                  "config": {
                    "single": "paragraph",
                    "label": "Promo text"
                  }
                },
                "image": {
                  "type": "Image",
                  "config": {
                    "label": "Promo image",
                    "thumbnails": [
                      {
                        "name": "32:15",
                        "width": 3200,
                        "height": 1500
                      },
                      {
                        "name": "16:9",
                        "width": 3200,
                        "height": 1800
                      },
                      {
                        "name": "square",
                        "width": 3200,
                        "height": 3200
                      }
                    ]
                  }
                },
                "link": {
                  "type": "Text",
                  "config": {
                    "label": "Link override"
                  }
                }
              }
            }
          }
        }
      }
    },
    "Metadata": {
      "metadataDescription": {
        "type": "StructuredText",
        "config": {
          "single": "paragraph,hyperlink,strong,em",
          "label": "Metadata description"
        }
      }
    },
    "Content relationships": {
      "series": {
        "type": "Group",
        "fieldset": "Event series",
        "config": {
          "fields": {
            "series": {
              "type": "Link",
              "config": {
                "label": "Series",
                "select": "document",
                "customtypes": ["event-series"]
              }
            }
          }
        }
      },
      "seasons": {
        "type": "Group",
        "fieldset": "Seasons",
        "config": {
          "fields": {
            "season": {
              "type": "Link",
              "config": {
                "label": "Season",
                "select": "document",
                "customtypes": ["seasons"],
                "placeholder": "Select a Season"
              }
            }
          }
        }
      },
      "parents": {
        "type": "Group",
        "fieldset": "Parents",
        "config": {
          "fields": {
            "order": {
              "type": "Number",
              "config": {
                "label": "Order"
              }
            },
            "parent": {
              "type": "Link",
              "config": {
                "label": "Parent",
                "select": "document",
                "customtypes": ["exhibitions"],
                "placeholder": "Select a parent"
              }
            }
          }
        }
      }
    }
  }
}
