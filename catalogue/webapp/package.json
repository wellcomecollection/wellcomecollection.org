    "@types/react": "^16.9.35",
    "@types/react-window": "^1.8.2",
    "@types/styled-components": "^5.1.15",
    "@types/supertest": "^2.0.11",
    "@weco/common": "1.0.0",
    "@weco/toggles": "1.0.0",
    "agentkeepalive": "^4.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "jest": "^27.3.1",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "lodash.clonedeep": "^4.5.0",
    "next": "^11.1.2",
