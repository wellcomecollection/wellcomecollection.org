{
  "name": "@weco/catalogue",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "NODE_ENV=development nodemon --watch server.ts --exec ts-node --project tsconfig.server.json server",
    "start": "NODE_ENV=production ts-node-transpile-only --project tsconfig.server.json server",
    "test": "NODE_ENV=test jest --no-cache",
    "test:watch": "yarn test -- --watchAll",
    "test:updateSnapshots": "yarn test -- --updateSnapshot",
    "deployBundleAnalysis": "aws s3 sync .dist s3://dash.wellcomecollection.org/bundles --only-show-errors --acl public-read"
  },
  "dependencies": {
    "@babel/core": "^7.1.5",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.0.2",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^18.14.6",
    "@types/react": "18.0.31",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.26",
    "@types/supertest": "^2.0.11",
    "@weco/common": "1.0.0",
    "@weco/toggles": "1.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "next": "^13.2.3",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.8",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "browserslist": [
    "last 2 versions",
    "iOS 8"
  ]
}
