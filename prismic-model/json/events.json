{
  "Event": {
    "title": {
      "type": "StructuredText",
      "config": {
        "label": "Title",
        "single": "heading1",
        "useAsTitle": true
      }
    },
    "format": {
      "type": "Link",
      "config": {
        "label": "Format",
        "select": "document",
        "customtypes": [
          "event-formats"
        ]
      }
    },
    "place": {
      "type": "Link",
      "fieldset": "Place",
      "config": {
        "select": "document",
        "customtypes": [
          "places"
        ],
        "label": "Where is it?"
      }
    },
    "series": {
      "type": "Group",
      "fieldset": "Event series",
      "config": {
        "fields": {
          "series": {
            "type": "Link",
            "config": {
              "label": "Series",
              "select": "document",
              "customtypes": [
                "event-series"
              ]
            }
          }
        }
      }
    },
    "times": {
      "type": "Group",
      "fieldset": "Times",
      "config": {
        "fields": {
          "startDateTime": {
            "type": "Timestamp",
            "config": {
              "label": "Start"
            }
          },
          "endDateTime": {
            "type": "Timestamp",
            "config": {
              "label": "End"
            }
          },
          "isFullyBooked": {
            "type": "Select",
            "config": {
              "label": "Fully booked",
              "options": [
                "yes"
              ]
            }
          }
        }
      }
    },
    "description": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph,hyperlink,strong,em,heading2,list-item",
        "label": "Description"
      }
    }
  },
  "Access": {
    "interpretations": {
      "type": "Group",
      "fieldset": "Interpretations",
      "config": {
        "fields": {
          "interpretationType": {
            "type": "Link",
            "config": {
              "label": "Interpretation",
              "select": "document",
              "customtypes": [
                "interpretation-types"
              ]
            }
          },
          "isPrimary": {
            "type": "Select",
            "config": {
              "label": "Primary interprtation",
              "options": [
                "yes"
              ]
            }
          }
        }
      }
    },
    "audiences": {
      "type": "Group",
      "fieldset": "Audiences",
      "config": {
        "fields": {
          "audience": {
            "type": "Link",
            "config": {
              "label": "Audience",
              "select": "document",
              "customtypes": [
                "audiences"
              ]
            }
          }
        }
      }
    }
  },
  "Reservation": {
    "bookingEnquiryTeam": {
      "type": "Link",
      "config": {
        "label": "Booking enquiry team",
        "select": "document",
        "customtypes": [
          "teams"
        ]
      }
    },
    "eventbriteEvent": {
      "type": "Embed",
      "config": {
        "label": "Eventbrite event"
      }
    },
    "isDropIn": {
      "type": "Select",
      "config": {
        "label": "Drop in",
        "options": [
          "yes"
        ]
      }
    },
    "bookingInformation": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph,hyperlink,strong,em",
        "label": "Event specific booking information"
      }
    },
    "cost": {
      "type": "Text",
      "config": {
        "label": "Cost"
      }
    }
  },
  "Schedule": {
    "schedule": {
      "type": "Group",
      "fieldset": "Events",
      "config": {
        "fields": {
          "event": {
            "type": "Link",
            "config": {
              "label": "Event",
              "select": "document",
              "customtypes": [
                "events"
              ]
            }
          }
        }
      }
    },
    "backgroundTexture": {
      "type": "Link",
      "config": {
        "label": "Background texture",
        "select": "document",
        "customtypes": [
          "background-textures"
        ]
      }
    }
  },
  "Contributors": {
    "contributors": {
      "type": "Group",
      "fieldset": "Contributors",
      "config": {
        "fields": {
          "role": {
            "type": "Link",
            "config": {
              "label": "Role",
              "select": "document",
              "customtypes": [
                "editorial-contributor-roles"
              ]
            }
          },
          "contributor": {
            "type": "Link",
            "config": {
              "label": "Contributor",
              "select": "document",
              "customtypes": [
                "people",
                "organisations"
              ]
            }
          },
          "description": {
            "type": "StructuredText",
            "config": {
              "multi": "paragraph,hyperlink,strong,em",
              "label": "Description of contribution"
            }
          }
        }
      }
    },
    "contributorsTitle": {
      "type": "StructuredText",
      "config": {
        "label": "Contributors heading override",
        "single": "heading1"
      }
    }
  },
  "Promo": {
    "promo": {
      "type": "Slices",
      "config": {
        "label": "Promo",
        "choices": {
          "editorialImage": {
            "type": "Slice",
            "fieldset": "Editorial image",
            "config": {
              "label": "Editorial image"
            },
            "non-repeat": {
              "caption": {
                "type": "StructuredText",
                "config": {
                  "label": "Promo text",
                  "single": "paragraph"
                }
              },
              "image": {
                "type": "Image",
                "config": {
                  "label": "Promo image",
                  "thumbnails": [
                    {
                      "name": "32:15",
                      "width": 3200,
                      "height": 1500
                    },
                    {
                      "name": "16:9",
                      "width": 3200,
                      "height": 1800
                    },
                    {
                      "name": "square",
                      "width": 3200,
                      "height": 3200
                    }
                  ]
                }
              },
              "link": {
                "type": "Text",
                "config": {
                  "label": "Link override"
                }
              }
            }
          }
        }
      }
    }
  }
}
